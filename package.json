  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.13",
    "bunup": "^0.16.25"
  },
  "peerDependencies": {
    "@elysiajs/eden": "^1.4.6",
    "@tanstack/react-query": "^5.90.20",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.22",
    "react": "^19.2.4",
    "typescript": "^5"
  },
  "dependencies": {
    "eden2query": "^0.2.0",
    "nanoid": "^5.1.6"
  }
